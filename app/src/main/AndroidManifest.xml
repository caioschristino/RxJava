<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.caios.rxexample">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SimpleExampleActivity"
            android:label="@string/simple_example"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".SimpleExampleWithFilterActivity"
            android:label="@string/simple_example_filter"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".SimpleExampleWithFilterAndMapActivity"
            android:label="@string/simple_example_filter_map"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>
</manifest>
